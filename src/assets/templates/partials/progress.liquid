{% if progress %}
  {% if progress.unpacking and progress.total %}
    <div class="alert warn">
      <i
        class="bi bi-folder-fill inline w-4 h-4 mr-2 text-amber-600 dark:text-amber-400 align-[-2px]"
        aria-hidden="true"
      ></i>
      Unpacking objects: {{ progress.processed }}/{{ progress.total }} ({{ progress.percent }}%)
    </div>
  {% elsif progress.queuedCount and progress.queuedCount > 0 %}
    <div class="alert">
      <i
        class="bi bi-clock inline w-4 h-4 mr-2 text-gray-600 dark:text-gray-300 align-[-2px]"
        aria-hidden="true"
      ></i>
      A push is queued and will start shortly.
    </div>
  {% endif %}
{% endif %}
