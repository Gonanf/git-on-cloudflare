{% layout 'layout' %}
{% block %}
{% render 'repo_nav', owner: owner, repo: repo, refEnc: refEnc %}
<h2>Commit {{ commitShort }}</h2>
<p>
  <strong>Author:</strong>
  {{ authorName }} &lt;{{ authorEmail }}&gt;
  <span class="muted">{{ when }}</span>
</p>
<p><strong>Parents:</strong> {{ parents | raw }}</p>
<p>
  <strong>Tree:</strong> <a href="/{{ owner }}/{{ repo }}/tree?ref={{ refEnc }}">{{ treeShort }}</a>
</p>
<pre>{{ message }}</pre>
{% endblock %}
